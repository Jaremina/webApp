<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>TPO2_PA_S6034</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	
	<context-param>
		<param-name>presentationServlet</param-name>
		<param-value>/ResultPresentationServlet</param-value>
	</context-param>
	
	<context-param>
		<param-name>getParametersServlet</param-name>
		<param-value>/GetParametersServlet</param-value>
	</context-param>
	
	<context-param>
		<param-name>resourceBundleServlet</param-name>
		<param-value>/ResourceBundleServlet</param-value>
	</context-param>
	
	<context-param>
		<param-name>commandClassName</param-name>
		<param-value>zad1.DataBaseAccess</param-value>
	</context-param>

	<context-param>
		<param-name>resourceBundleName</param-name>
		<param-value>zad1.Messages</param-value>
	</context-param>
	
	<context-param>
		<param-name>dbName</param-name>
		<param-value>jdbc/ksidb</param-value>
	</context-param>
	
</web-app>